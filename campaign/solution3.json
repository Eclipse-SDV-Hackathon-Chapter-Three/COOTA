{
    "metadata": {
        "name": "payload-v-3",
        "namespace": "default"
    },
    "spec": {
        "displayName": "Fleet Management Payload",
        "rootResource": "payload3",
        "version": "1",
        "components": [
            {
                "name": "payload3",
                "type": "ankaios",
                "properties": {
                    "ankaios.runtime": "podman",
                    "ankaios.restartPolicy": "ALWAYS",
                    "ankaios.runtimeConfig": "image: localhost:5000/payload-3:latest\ncommandOptions: [\"-v\", \"/tmp/vehicle-vin:/tmp/vehicle-vin:ro\"]"
                }
            }
        ]
    }
}
